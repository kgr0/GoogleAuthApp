@model IEnumerable<GoogleAuthApp.Models.PictureViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Мои изображения</h2>

<p>
    @Html.ActionLink("Add Picture", "Create")
</p>


    @foreach (var item in Model)
    {
                @Html.Raw("<img style='width:200px; height:150px;' src=\"data:image/jpeg;base64,"
                                    + Convert.ToBase64String(item.Image) + "\" />")
    }
