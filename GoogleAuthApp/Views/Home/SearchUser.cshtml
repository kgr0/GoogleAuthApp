
@{
    ViewBag.Title = "SearchUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <link href="@Url.Content("~/Styles/SearchUser.css")" rel="stylesheet" type="text/css" />
</head>
<h2>Search Results</h2>

<table>
    @foreach (var user in ViewBag.List)
    {
<tr>
    <td>
        @Html.Raw("<img src=\"data:image/jpeg;base64,"
                                          + Convert.ToBase64String(user.Photo) + "\" />")
    </td>
    <td>@user.UserName</td>
    <td>@user.FirstName</td>
    <td>@user.SecondName</td>
</tr>
    }
</table>
