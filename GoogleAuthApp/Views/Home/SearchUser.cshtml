@{
    ViewBag.Title = "SearchUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <link href="@Url.Content("~/Styles/SearchUser.css")" rel="stylesheet" type="text/css" />
</head>
<h2>Search Results</h2>


    @foreach (var user in ViewBag.List)
    {
    <div style="position:relative;" class="searchedItem">
        <a href="@Url.Action("ShowProfile", "Profile", new { userId = user.UserId} )" style="height: 200px; width: 200px;position:relative;">
           
                <span>
                    @Html.Raw("<img src=\"data:image/jpeg;base64,"
                                           + Convert.ToBase64String(user.Photo) + "\" />")
                </span>
                <span>@user.UserName</span>
                <span>@user.FirstName</span>
                <span>@user.SecondName</span>
        </a>
    </div>
    }
